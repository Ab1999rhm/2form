{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/views/index.html"
    },
    {
      "source": "/view-registrations",
      "destination": "/views/view-registration.html"
    },
    {
      "source": "/(.*)",
      "destination": "/views/index.html"
    }
  ]
}
